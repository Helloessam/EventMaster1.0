<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="804.0" prefWidth="1175.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="clientgui.EntrySceneController">
   <children>
      <ImageView fitHeight="804.0" fitWidth="1200.0" pickOnBounds="true" smooth="false">
         <image>
            <Image url="@background.jpg" />
         </image>
      </ImageView>
      <VBox alignment="CENTER" layoutX="254.0" layoutY="147.0" prefHeight="511.0" prefWidth="693.0" spacing="50.0">
         <children>
            <ImageView fitHeight="250.0" fitWidth="500.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@event_picture_1.png" />
               </image>
               <VBox.margin>
                  <Insets top="50.0" />
               </VBox.margin>
            </ImageView>
            <VBox alignment="CENTER" prefHeight="213.0" prefWidth="693.0" spacing="30.0">
               <children>
                  <Button fx:id="logInBtn" alignment="CENTER" layoutX="34.0" layoutY="10.0" mnemonicParsing="false" onAction="#logInBtn" onMouseClicked="#logInBtn" text="Log In to Your Account" textAlignment="CENTER">
                     <font>
                        <Font name="Cambria" size="31.0" />
                     </font>
                  </Button>
                  <Button fx:id="registerBtn" alignment="CENTER" contentDisplay="CENTER" layoutX="10.0" layoutY="121.0" mnemonicParsing="false" onAction="#registerBtn" text="Register a New Account" textAlignment="CENTER">
                     <font>
                        <Font name="Cambria" size="31.0" />
                     </font>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
